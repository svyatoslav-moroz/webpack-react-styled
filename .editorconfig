indent_size=2

[*.less]
indent_style=space
indent_size=2

[*.scss]
indent_style=space
indent_size=2

[{jshint.json,*.jshintrc}]
indent_style=space
indent_size=2

[tsconfig.json]
indent_style=space
indent_size=2

[*.js.map]
indent_style=space
indent_size=2

[.eslintrc]
indent_style=space
indent_size=2

